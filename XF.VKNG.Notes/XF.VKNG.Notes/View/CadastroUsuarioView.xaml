<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF.VKNG.Notes.View.CadastroUsuarioView">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Entry x:Name="txtEmail" Placeholder="E-mail" Keyboard="Email" />
            <Entry x:Name="txtPassword" Placeholder="Password" IsPassword="True" />
            <Entry x:Name="txtConfirmPassword" Placeholder="Confirmar Password" IsPassword="True" />
            <Button x:Name="btnRegister" Text="Cadastrar" Clicked="btnRegister_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>